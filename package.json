    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "lovable-tagger": "^1.1.7",
    "postcss": "^8.4.47",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.11",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  }
}
